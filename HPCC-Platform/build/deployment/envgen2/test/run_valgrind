#!/bin/bash

valgrind --tool=memcheck --leak-check=yes --leak-check=full --show-leak-kinds=all /home/sathvik/hpcc/HPCC-Platform/build/Debug/bin/envgen2  -env-in /home/sathvik/hpcc/HPCC-Platform/build/initfiles/etc/DIR_NAME/environment.xml -env-out /tmp/envgen2_test/roxie_add_ip_1.xml -add-node roxie#myroxie@ip=1.0.0.22 > /tmp/envgen2_test/roxie_add_ip_1.vg 2>&1
echo "check /tmp/envgen2_test/roxie_add_ip_1.vg"
