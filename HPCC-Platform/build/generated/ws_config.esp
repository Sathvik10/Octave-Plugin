{
	CHttpRequest *request=static_cast<CHttpRequest*>(ireq);
	IProperties *props = (request) ? request->queryParameters() : NULL;

	if (!stricmp(method, "GetConfigAccess") || !stricmp(method, "ConfigAccessRequest"))
